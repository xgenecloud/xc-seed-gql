{
  "name": "xc-seed-graphql",
  "version": "1.0.0",
  "description": "abc",
  "main": "server/app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon server/app.js",
    "install1": "npm install",
    "test": "NODE_ENV=test mocha ./server/tests/router/resolvers.test.js  && exit 1",
    "docs": "rm -rf docs/* && ./node_modules/.bin/jsdoc -c jsdoc.json && open docs/index.html",
    "xc": "npm install --save xc-core@latest xc-data-mapper@latest xc-migrator@latest",
    "lint": "eslint \"server/**\" --config .eslintrc.js",
    "gcp:fn": "gcloud functions deploy xc-app-gql-function --runtime nodejs10 --trigger-http --entry-point app",
    "gcp:cloud:build": "gcloud builds submit --tag gcr.io/PROJECT-ID/xc-gql-image",
    "gcp:cloud:run": "gcloud run deploy --image gcr.io/PROJECT-ID/xc-gql-image  --platform managed  --add-cloudsql-instances INSTANCE_CONNECTION_NAME --update-env-vars INSTANCE_CONNECTION_NAME=\"INSTANCE_CONNECTION_NAME\"",
    "azure:start": "func start",
    "azure:deploy": "func azure functionapp publish xc-azure-function",
    "zeit:now": "now && now alias",
    "aws:lambda:zip": "npm prune --production ; rm lambda.zip ; zip -r -o lambda.zip .",
    "aws:lambda:zip:win": "npm prune --production ; del /f lambda.zip ; powershell Compress-Archive . lambda.zip",
    "ali:fn:compute": "fun deploy"
  },
