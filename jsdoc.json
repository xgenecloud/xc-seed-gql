{
  "source": {
    "include": ["./server/xgenecloud/xc-server"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|help|static)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./readme.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/@pixi/jsdoc-template"
  }
}